{
  "expo": {
    "name": "Lucas Fii Research",
    "slug": "lucas-fii-research-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "Aplicativo mobile do Lucas Fii Research - Plataforma de pesquisa e an√°lises",
    "splash": {
      "image": "./assets/logo_2.png",
      "resizeMode": "contain",
      "backgroundColor": "#060d6f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucasfiiresearch.mobile",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#060d6f"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lucasfiiresearch.mobile",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "expo": {
      "developer": {
        "tool": "expo-cli"
      }
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "your-expo-project-id"
      }
    }
  }
}
